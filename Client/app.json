{
  "expo": {
    "name": "FootBall-ReactNative",
    "slug": "FootBall-ReactNative",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon.png",
    "scheme": "footballreactnative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sam.megh0305.FootBallReactNative"
    },
    "platforms": ["ios", "android"],
    "plugins": [
      "expo-router",
      [
        "@stripe/stripe-react-native",
        {
          "enableGooglePay": true
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "915a8a9c-3532-4707-910e-555c201bafd1"
      }
    }
  }
}
